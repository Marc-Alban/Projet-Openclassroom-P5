{% extends 'Backend/layout.html.twig' %}
{#---------------------------BLOC-Title-----------------------------#}
{% block head %} <title> Page insertion </title> {% endblock %}
{#---------------------------BLOC-Title-----------------------------#}
{% block content %}
<div class="container">
    {#---------------------------SECTION-----------------------------#}
    <section class="section team-section mt-3">
        <div class="row">
            <div class="col-12">
                <div class="row">
                    <div class="col-md-12 mb-1">
                        {#---------------------------START-TABLE-CONTENT-----------------------------#}
                        <div class="classic-tabs">
                            <div class="tab-content card">
                                <div class="tab-pane fade active show"
                                    id="panel83" role="tabpanel">
                                    <div class="table-responsive">
                                        {#---------------------------TABLE-----------------------------#}
                                        <table class="table">
                                            {#---------------------------HEAD-TABLE-----------------------------#}
                                            <thead>
                                                <tr>
                                                    <th>
                                                        Titre
                                                    </th>
                                                    <th>
                                                        Contenu
                                                    </th>
                                                    <th>
                                                        Modification
                                                    </th>
                                                    <th>
                                                        Supprimer
                                                    </th>
                                                </tr>
                                            </thead>
                                            {#---------------------------END-HEAD-Table-----------------------------#}
                                            {#---------------------------BODY-Table-----------------------------#}
                                            <tbody>
                                                {% if data.partenaire %}
                                                {% for partenaires in
                                                data.partenaire %}
                                                <tr>
                                                    <th>
                                                        {{partenaires.image}}
                                                    </th>
                                                    <td>
                                                        {{partenaires.legende}}
                                                    </td>
                                                    <td>
                                                        <a class="teal-text"
                                                            data-toggle="tooltip"
                                                            data-placement="top"
                                                            title="Update"><i
                                                                class="fas
                                                                fa-pencil-alt"></i></a>
                                                    </td>
                                                    <td>
                                                        <a class="red-text"
                                                            data-toggle="tooltip"
                                                            data-placement="top"
                                                            title="Delete"><i
                                                                class="fas
                                                                fa-times"></i></a>
                                                    </td>
                                                </tr>
                                                {% endfor %}
                                                {% else %}
                                                <tr>
                                                    <th>
                                                    </th>
                                                    <td>
                                                    </td>
                                                    <td>
                                                        <a class="teal-text"
                                                            data-toggle="tooltip"
                                                            data-placement="top"
                                                            title="Update"><i
                                                                class="fas
                                                                fa-pencil-alt"></i></a>
                                                    </td>
                                                    {% block table %}
                                                    <td>
                                                        <a class="red-text"
                                                            data-toggle="tooltip"
                                                            data-placement="top"
                                                            title="Delete"><i
                                                                class="fas
                                                                fa-times"></i></a>
                                                    </td>
                                                    {% endblock %}
                                                </tr>
                                                {% endif %}
                                            </tbody>
                                            {#---------------------------END-BODY-Table-----------------------------#}
                                        </table>
                                        {#---------------------------END-TABLE-----------------------------#}
                                    </div>
                                </div>
                            </div>
                            {#---------------------------END-START-TABLE-CONTENT-----------------------------#}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    {#---------------------------END-SECTION-----------------------------#}
</div>
{% endblock %}