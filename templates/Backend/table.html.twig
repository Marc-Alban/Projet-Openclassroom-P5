{% extends 'Backend/layout.html.twig' %}
{% block head %} <title> Page insertion </title> {% endblock %}
{% block content %}
<div class="container">
    <!-- Section: Customers -->
    <section class="section team-section mt-3">
        <!-- First row -->
        <div class="row">
            <!-- First column -->
            <div class="col-12">
                <div class="row">
                    <div class="col-md-12 mb-1">
                        <!-- Tabs -->
                        <div class="classic-tabs">
                            <!-- Tab panels -->
                            <div class="tab-content card">
                                <!-- Panel 1 -->
                                <div class="tab-pane fade active show"
                                    id="panel83" role="tabpanel">
                                    <div class="table-responsive">
                                        <table class="table">
                                            <thead>
                                                <tr>
                                                    <th>
                                                        Titre
                                                    </th>
                                                    <th>
                                                        Contenu
                                                    </th>
                                                    <th>
                                                        Modification
                                                    </th>
                                                    <th>
                                                        Supprimer
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {% if data.partenaire %}
                                                {% for partenaires in
                                                data.partenaire %}
                                                <tr>
                                                    <th>
                                                        {{partenaires.image}}
                                                    </th>
                                                    <td>
                                                        {{partenaires.legende}}
                                                    </td>
                                                    <td>
                                                        <a class="teal-text"
                                                            data-toggle="tooltip"
                                                            data-placement="top"
                                                            title="Update"><i
                                                                class="fas
                                                                fa-pencil-alt"></i></a>
                                                    </td>
                                                    <td>
                                                        <a class="red-text"
                                                            data-toggle="tooltip"
                                                            data-placement="top"
                                                            title="Delete"><i
                                                                class="fas
                                                                fa-times"></i></a>
                                                    </td>
                                                </tr>
                                                {% endfor %}
                                                {% else %}
                                                <tr>
                                                    <th>
                                                    </th>
                                                    <td>
                                                    </td>
                                                    <td>
                                                        <a class="teal-text"
                                                            data-toggle="tooltip"
                                                            data-placement="top"
                                                            title="Update"><i
                                                                class="fas
                                                                fa-pencil-alt"></i></a>
                                                    </td>
                                                    {% block table %}
                                                    <td>
                                                        <a class="red-text"
                                                            data-toggle="tooltip"
                                                            data-placement="top"
                                                            title="Delete"><i
                                                                class="fas
                                                                fa-times"></i></a>
                                                    </td>
                                                    {% endblock %}
                                                </tr>
                                                {% endif %}
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <!-- /.Panel 1 -->
                            </div>
                            <!-- /.Tabs -->
                        </div>
                    </div>
                </div>
            </div>
            <!-- First column -->
        </div>
        <!-- First row -->
    </section>
    <!-- Section: Customers -->
</div>
{% endblock %}