<!DOCTYPE html>
<html lang="en">
  <head>
    {#----------------------------Encodage-UTF-8---------------------------#}
    <meta charset="UTF-8" />
    {#----------------------------Responsive-VIEWPORT-CSS---------------------------#}
    <meta name="viewport" content="width=device-width, initial-scale=1,
      shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    {% block head %} {% endblock %}
    {#----------------------------BM-ICON----------------------------#}
    <link rel="icon" href="img/bm-favicon.ico" type="image/x-icon">
    {#----------------------------Font-Awesome----------------------------#}
    <link rel="stylesheet"
      href="https://use.fontawesome.com/releases/v5.7.0/css/all.css">
    {#----------------------------Material-Design-Bootstrap----------------------------#}
    <link href="css/MDB-Free/bootstrap.min.css" rel="stylesheet">
    <link href="css/MDB-Free/mdb.min.css" rel="stylesheet">
    {#----------------------------Style-Css---------------------------#}
    <link href="css/frontend.css" rel="stylesheet">
    {#----------------------------JS-RECAPTCHA---------------------------#}
    <script type="text/javascript">
      var verifyCallback = function (response) {
        alert('ok');
      };
      var onloadCallback = function () {
        grecaptcha.render('captcha', {
          'sitekey': '6LdCHMIUAAAAAAWzKdTsmXFOtMohGlCSx7BXVM-9',
          'callback': verifyCallback,
          'theme': 'dark'
        });
      };
    </script>
    {#---------------------------END-JS-RECAPTCHA---------------------------#}
  </head>
  <body class="fixed-sn homepage-v3">
    {#----------------------------HEADER----------------------------#}
    <header class="navbar-grey">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 d-flex" id="haut">
            {#----------------------------HEADER-LOGO---------------------------#}
            <a class="navbar-brand mr-auto p-2" href="?p=home"><span>BM</span>Finance</a>
            {#----------------------------MODAL----------------------------#}
            <div id="user" class="p-2" data-toggle="modal"
              data-target="#UserLog">
              <i class="fas fa-user"></i>
            </div>
            <div class="modal fade" id="UserLog" tabindex="-1" role="dialog"
              aria-labelledby="TitleModalUser"
              aria-hidden="true">
              <div class="modal-dialog" role="document">
                {#----------------------------MODAL-CONTENT----------------------------#}
                <div class="modal-content form-elegant">
                  {#----------------------------MODAL-HEADER----------------------------#}
                  <div class="modal-header text-center">
                    <h3 class="modal-title w-100 dark-grey-text font-weight-bold
                      my-3" id="TitleModalUser"><strong>Acces Administration</strong></h3>
                    <button type="button" class="close" data-dismiss="modal"
                      aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  {#----------------------------MODAL-CONTENT----------------------------#}
                  <div class="modal-body mx-4">
                    <form action="/?p=dashboard&action=connexion" method="POST">
                      <div class="md-form mb-5">
                        {#----------------------------MODAL-PSEUDO----------------------------#}
                        <input type="text" id="Form-User" name="pseudo"
                          class="form-control
                          validate" value="">
                        <label data-error="wrong" data-success="right"
                          for="Form-User">Identifiant</label>
                      </div>
                      {#----------------------------MODAL-PASSWORD----------------------------#}
                      <div class="md-form pb-3">
                        <input type="password" id="Form-pass" name="password"
                          class="form-control
                          validate">
                        <label data-error="wrong" data-success="right"
                          for="Form-pass">Mot de passe</label>
                        {#----------------------------MODAL-PASSWORD-FORGET---------------------------#}
                        <p class="font-small red-text d-flex
                          justify-content-start"><a href="?p=lostPassword"
                            class="red-text
                            ml-1">*Mot de passe oubli√©</a></p>
                      </div>
                      {#----------------------------MODAL-HIDDEN----------------------------#}
                      <div class="md-form">
                        <input type="hidden" name="token" id="token"
                          value="{{session.token}}" />
                      </div>
                      {#----------------------------MODAL-RECAPTCHA----------------------------#}
                      <div id="captcha"></div>
                      {#----------------------------MODAL-SUBMIT----------------------------#}
                      <div class="text-center mb-3">
                        <input type="submit" name="connexion" class="btn
                          btn-block btn-red
                          btn-rounded z-depth-1a" value="Je me connect">
                      </div>
                    </form>
                  </div>
                </div>
                {#---------------------------END-MODAL-CONTENT----------------------------#}
              </div>
            </div>
            {#---------------------------END-MODAL-----------------------------#}
          </div>
        </div>
      </div>
      {#---------------------------HEADER-CONTENT----------------------------#}
      <div class="container">
        <div class="row">
          {#---------------------------NAVBAR-CONTENT----------------------------#}
          <nav class="navbar navbar-expand-md">
            <button class="navbar-toggler toggler-example" type="button"
              data-toggle="collapse"
              data-target="#navbarSupportedContent"
              aria-controls="navbarSupportedContent" aria-expanded="false"
              aria-label="Toggle navigation"><span class="dark-blue-text"><i
                  class="fas fa-bars fa-1x"></i></span></button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="navbar-nav mr-auto">
                {#---------------------------NAVBAR-HOME----------------------------#}
                <li class="nav-item">
                  <a class="nav-link green" href="?p=home">Accueil</a>
                </li>
                {#--------------------------ONGLET-NAVBAR-BLOG----------------------------#}
                <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle blue"
                    id="navbarDropdownMenuLink1" data-toggle="dropdown"
                    aria-haspopup="true" aria-expanded="false">Blog</a>
                  <div class="dropdown-menu dropdown-primary"
                    aria-labelledby="navbarDropdownMenuLink">
                    {#---------------------------NAVBAR-SOUS-MENU-BLOG---------------------------#}
                    <a class="dropdown-item" href="?p=article">Article de la
                      semaine</a>
                    <a class="dropdown-item" href="?p=listesArticles&pp=1">Liste
                      des
                      articles
                      hebdomadaire</a>
                  </div>
                </li>
                {#-------------------------END-ONGLET-NAVBAR-BLOG----------------------------#}
                {#-------------------------ONGLET-NAVBAR-QUI-SOMMES-NOUS-?----------------------------#}
                <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle orange"
                    id="navbarDropdownMenuLink1" data-toggle="dropdown"
                    aria-haspopup="true" aria-expanded="false">Qui sommes-nous ?</a>
                  <div class="dropdown-menu dropdown-primary"
                    aria-labelledby="navbarDropdownMenuLink">
                    {#---------------------------NAVBAR-SOUS-MENU-QUI-SOMMES-NOUS-?---------------------------#}
                    <a class="dropdown-item" href="?p=page&title=nosvaleurs">Nos
