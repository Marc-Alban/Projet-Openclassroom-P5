                    <a class="dropdown-item" href="?p=page&title=adresse">Adresse</a>
                  </div>
                </li>
                {#------------------------END-ONGLET-NAVBAR-QUI-SOMMES-NOUS-?----------------------------#}
                {#-------------------------ONGLET-NAVBAR-NOTRE-EXPERTISE----------------------------#}
                <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle yellow"
                    id="navbarDropdownMenuLink1" data-toggle="dropdown"
                    aria-haspopup="true" aria-expanded="false">Notre expertise</a>
                  <div class="dropdown-menu dropdown-primary"
                    aria-labelledby="navbarDropdownMenuLink">
                    {#---------------------------NAVBAR-SOUS-MENU-NOTRE-EXPERTISE---------------------------#}
                    <a class="dropdown-item"
                      href="?p=page&title=conseilpatrimonial">Conseil
                      patrimonial</a>
                    <a class="dropdown-item"
                      href="?p=page&title=conseileninvestissementfinancier">Conseil
                      en
                      investissement
                      financier</a>
                    <a class="dropdown-item"
                      href="?p=page&title=conseileninvestissementimmobilier">Conseil
                      en
                      investissement
                      immobilier</a>
                    <a class="dropdown-item"
                      href="?p=page&title=autressolutionsd'investissement">Autres
                      solutions
                      d‚Äôinvestissement</a>
                  </div>
                </li>
                {#--------------------------END-NAVBAR-SOUS-MENU-NOTRE-EXPERTISE---------------------------#}
                {#------------------------END-ONGLET-NAVBAR-NOTRE-EXPERTISE----------------------------#}
                {#------------------------ONGLET-NAVBAR-CONTACT----------------------------#}
                <li class="nav-item">
                  <a class="nav-link blueStrong" href="?p=contact">Contact</a>
                </li>
                {#-----------------------END-ONGLET-NAVBAR-CONTACT----------------------------#}
              </ul>
            </div>
          </nav>
          {#-----------------------END-NAVBAR----------------------------#}
        </div>
      </div>
    </header>
    {#-----------------------END-HEADER----------------------------#}
    {#-----------------------MAIN----------------------------#}
    <main>
      <div class="container-fluid">
        <div class="row mt-5">
          <div class="container">
            <div class="row">
              {#-----------------------MAIN-CONTENT-----------------------------#}
              <div class="col-xl-10 col-lg-10 col-md-9">
                <section class="section extra-margins listing-section">
                  {#-----------------------MAIN-CONTENT-TITLE----------------------------#}
                  <h4 class="font-weight-bold">
                    <strong>
                      {% block title %} {% endblock %}
                    </strong>
                  </h4>
                  <hr class="red title-hr">
                  {#-----------------------MAIN-CONTENT-DESCRIPTION----------------------------#}
                  {% block content %}
                  {% endblock %}
                </section>
              </div>
              {#----------------------END-MAIN-CONTENT-----------------------------#}
              {#-----------------------SIDEBAR-----------------------------#}
              <div class="col-12 col-md-3 col-lg-2 col-xl-2 widget-column mt-0">
                <section class="margeAside mb-5">
                  {#-----------------------SIDEBAR-TITLE-FIRST----------------------------#}
                  <h5 class="font-weight-bold aside">PARTENAIRES</h5>
                  {#-----------------------SIDEBAR-CONTENT-FIRST----------------------------#}
                  <div class="d-flex justify-content-center">
                    <div class="col-7 col-sm-4 col-md-10 col-lg-12 col-xl-12">
                      <div class="row mt-4">
                        <div class="m-auto col-8 col-sm-8 col-md-12 col-lg-12">
                          {#-----------------------SIDEBAR-CONTENT-FIRST-IMAGE---------------------------#}
                          <div class="view overlay z-depth-1">
                            <img src="img/{{session.partenaire.data.image}}"
                              class="img-fluid"
                              alt="Post">
                            <a href="#!">
                              <div class="mask rgba-white-slight waves-effect
                                waves-light"></div>
                            </a>
                          </div>
                          {#-----------------------SIDEBAR-CONTENT-FIRST-LINK---------------------------#}
                          <div class="text-center mt-3">
                            <p class="mb-1 font-weight-bold">
                              <a href="#!" class="text-hover">{{session.partenaire.data.legende}}</a>
                            </p>
                          </div>
                        </div>
                        {#-----------------------SIDEBAR-CONTENT-FIRST-BLOCK-CONTENT--------------------------#}
                        <div class="m-auto col-8 col-sm-8 col-md-12
                          col-lg-12">
                          {#-----------------------SIDEBAR-CONTENT-FIRST-IMAGE---------------------------#}
                          <div class="view overlay z-depth-1">
                            <img src="img/quantalys.jpg" class="img-fluid"
                              alt="Post">
                            <a href="#!">
                              <div class="mask rgba-white-slight waves-effect
                                waves-light"></div>
                            </a>
                          </div>
                          {#-----------------------SIDEBAR-CONTENT-FIRST-LINK---------------------------#}
                          <div class="text-center mt-3">
                            <p class="mb-1 font-weight-bold">
                              <a href="#!" class="text-hover">Lien</a>
                            </p>
                          </div>
                        </div>
                        {#----------------------END-SIDEBAR-CONTENT-FIRST-BLOCK-CONTENT--------------------------#}
                      </div>
                      {#----------------------END-SIDEBAR-CONTENT-FIRST----------------------------#}
                    </div>
                  </div>
                  {#----------------------END-SIDEBAR-CONTENT-FIRST----------------------------#}
                </section>
                {#-----------------------BLOCK-SIDEBAR-CLASSIFICATION-YEARS-POST-------------------------#}
                {% block archive %}
                {% endblock %}
                {#----------------------END-BLOCK-SIDEBAR-CLASSIFICATION-YEARS-POST-------------------------#}
              </div>
              {#-----------------------END-SIDEBAR-------------------------#}
            </div>
          </div>
        </div>
      </div>
    </main>
