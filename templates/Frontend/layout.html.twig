<!DOCTYPE html>
<html lang="en">
  <head>
    {#----------------------------Encodage-UTF-8---------------------------#}
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    {#----------------------------Responsive-VIEWPORT-CSS---------------------------#}
    <meta name="viewport" content="width=device-width, initial-scale=1,
      shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    {% block head %} {% endblock %}
    {#----------------------------BM-ICON----------------------------#}
    <link rel="icon" href="img/bm-favicon.ico" type="image/x-icon">
    {#----------------------------Font-Awesome----------------------------#}
    <link rel="stylesheet"
      href="https://use.fontawesome.com/releases/v5.7.0/css/all.css">
    {#----------------------------Material-Design-Bootstrap----------------------------#}
    <link href="css/MDB-Free/bootstrap.min.css" rel="stylesheet">
    <link href="css/MDB-Free/mdb.min.css" rel="stylesheet">
    {#----------------------------Style-Css---------------------------#}
    <link href="css/frontend.css" rel="stylesheet">
    {#----------------------------Captcha---------------------------#}
    <script src="https://www.google.com/recaptcha/api.js" async defer></script>
  </head>
  <body class="fixed-sn homepage-v3">
    {#----------------------------HEADER----------------------------#}
    <header class="navbar-grey">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 d-flex" id="haut">
            {#----------------------------HEADER-LOGO---------------------------#}
            <a class="navbar-brand mr-auto p-2" href="?p=home"><span>LES 3
                SOLDATS BLANCS</span></a>
            {% if session.user %}
            <a href="http://projet5.marcalban.fr/?p=dashboard&action=connexion"
              class="login"><button
                type="button" class="btn btn-red"><i class="fas fa-user"></i>
                | {{session.user}}</button></a>
            {% else %}
            {#----------------------------MODAL----------------------------#}
            <div id="user" class="p-2" data-toggle="modal"
              data-target="#UserLog">
              <i class="fas fa-user"></i>
            </div>
            <div class="modal fade" id="UserLog" tabindex="-1" role="dialog"
              aria-labelledby="TitleModalUser"
              aria-hidden="true">
              <div class="modal-dialog" role="document">
                {#----------------------------MODAL-CONTENT----------------------------#}
                <div class="modal-content form-elegant">
                  {#----------------------------MODAL-HEADER----------------------------#}
                  <div class="modal-header text-center">
                    <h3 class="modal-title w-100 dark-grey-text font-weight-bold
                      my-3" id="TitleModalUser"><strong>Acces Administration</strong></h3>
                    <button type="button" class="close" data-dismiss="modal"
                      aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  {#----------------------------MODAL-CONTENT----------------------------#}
                  <div class="modal-body mx-4">
                    <form action="/?p=dashboard&action=connexion" method="POST">
                      <div class="md-form mb-5">
                        {#----------------------------MODAL-PSEUDO----------------------------#}
                        <input type="text" id="Form-User" name="pseudo"
                          class="form-control
                          validate" value="">
                        <label data-error="wrong" data-success="right"
                          for="Form-User">Identifiant</label>
                      </div>
                      {#----------------------------MODAL-PASSWORD----------------------------#}
                      <div class="md-form pb-3">
                        <input type="password" id="Form-pass" name="password"
                          class="form-control
                          validate">
                        <label data-error="wrong" data-success="right"
                          for="Form-pass">Mot de passe</label>
                        {#----------------------------MODAL-PASSWORD-FORGET---------------------------#}
                        <p class="font-small red-text d-flex
                          justify-content-start"><a href="?p=lostPassword"
                            class="red-text
                            ml-1">*Mot de passe oubli√©</a></p>
                      </div>
                      {#----------------------------MODAL-HIDDEN----------------------------#}
                      <div class="md-form">
                        <input type="hidden" name="token" id="token"
                          value="{{session.token}}" />
                      </div>
                      {#----------------------------MODAL-RECAPTCHA----------------------------#}
                      <div class="g-recaptcha"
                        data-sitekey="6LcqetMUAAAAAOHdIdq_iOJ8n6EhYnfZGXMam5T2"></div>
                      <br/>
                        {#----------------------------MODAL-SUBMIT----------------------------#}
                        <div class="text-center mb-3">
                          <input type="submit" name="connexion" class="btn
                            btn-block btn-red
                            btn-rounded z-depth-1a" value="Je me connect">
                        </div>
                      </form>
                    </div>
                  </div>
                  {#---------------------------END-MODAL-CONTENT----------------------------#}
                </div>
              </div>
              {#---------------------------END-MODAL-----------------------------#}
              {% endif %}
            </div>
          </div>
        </div>
        {#---------------------------HEADER-CONTENT----------------------------#}
        <div class="container">
          <div class="row">
            {#---------------------------NAVBAR-CONTENT----------------------------#}
            <nav class="navbar navbar-expand-md">
              <button class="navbar-toggler toggler-example" type="button"
                data-toggle="collapse"
                data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation"><span class="dark-blue-text"><i
                    class="fas fa-bars fa-1x"></i></span></button>
              <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                  {#---------------------------NAVBAR-HOME----------------------------#}
                  <li class="nav-item">
                    <a class="nav-link green" href="?p=home">Accueil</a>
                  </li>
                  {#--------------------------ONGLET-NAVBAR-BLOG----------------------------#}
                  <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle blue"
                      id="navbarDropdownMenuLink1" data-toggle="dropdown"
                      aria-haspopup="true" aria-expanded="false">Blog</a>
                    <div class="dropdown-menu dropdown-primary"
                      aria-labelledby="navbarDropdownMenuLink">
                      {#---------------------------NAVBAR-SOUS-MENU-BLOG---------------------------#}
                      <a class="dropdown-item" href="?p=article">Article de la
                        semaine</a>
                      <a class="dropdown-item" href="?p=listesArticles&pp=1">Liste
                        des
                        articles
                        hebdomadaire</a>
                    </div>
                  </li>
                  {#-------------------------END-ONGLET-NAVBAR-BLOG----------------------------#}
                  {#-------------------------ONGLET-NAVBAR-QUI-SOMMES-NOUS-?----------------------------#}
                  <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle orange"
                      id="navbarDropdownMenuLink1" data-toggle="dropdown"
                      aria-haspopup="true" aria-expanded="false">Qui sommes-nous
                      ?</a>
                    <div class="dropdown-menu dropdown-primary"
                      aria-labelledby="navbarDropdownMenuLink">
                      {#---------------------------NAVBAR-SOUS-MENU-QUI-SOMMES-NOUS-?---------------------------#}
                      <a class="dropdown-item"
                        href="?p=page&title={{session.titlePageLink[0]}}">{{session.titlePage[0]}}</a>
                      <a class="dropdown-item"
                        href="?p=page&title={{session.titlePageLink[1]}}">{{session.titlePage[1]}}</a>
                    </div>
                  </li>
                  {#------------------------END-ONGLET-NAVBAR-QUI-SOMMES-NOUS-?----------------------------#}
                  {#-------------------------ONGLET-NAVBAR-NOTRE-EXPERTISE----------------------------#}
                  <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle yellow"
                      id="navbarDropdownMenuLink1" data-toggle="dropdown"
                      aria-haspopup="true" aria-expanded="false">Notre expertise</a>
                    <div class="dropdown-menu dropdown-primary"
                      aria-labelledby="navbarDropdownMenuLink">
                      {#---------------------------NAVBAR-SOUS-MENU-NOTRE-EXPERTISE---------------------------#}
                      <a class="dropdown-item"
                        href="?p=page&title={{session.titlePageLink[2]}}">{{session.titlePage[2]}}</a>
                      <a class="dropdown-item"
                        href="?p=page&title={{session.titlePageLink[3]}}">{{session.titlePage[3]}}</a>
                      <a class="dropdown-item"
                        href="?p=page&title={{session.titlePageLink[4]}}">{{session.titlePage[4]}}</a>
                      <a class="dropdown-item"
                        href="?p=page&title={{session.titlePageLink[5]}}">{{session.titlePage[5]}}</a>
                    </div>
                  </li>
                  {#--------------------------END-NAVBAR-SOUS-MENU-NOTRE-EXPERTISE---------------------------#}
                  {#------------------------END-ONGLET-NAVBAR-NOTRE-EXPERTISE----------------------------#}
                  {#------------------------ONGLET-NAVBAR-CONTACT----------------------------#}
                  <li class="nav-item">
                    <a class="nav-link blueStrong"
                      href="?p={{session.titlePageLink[6]}}">{{session.titlePage[6]}}</a>
                  </li>
                  {#-----------------------END-ONGLET-NAVBAR-CONTACT----------------------------#}
                </ul>
              </div>
            </nav>
            {#-----------------------END-NAVBAR----------------------------#}
          </div>
        </div>
      </header>
      {#-----------------------END-HEADER----------------------------#}
      {#-----------------------MAIN----------------------------#}
      <main>
        <div class="container-fluid">
          <div class="row mt-5">
            <div class="container">
              <div class="row">
                {#-----------------------MAIN-CONTENT-----------------------------#}
                <div class="col-xl-10 col-lg-10 col-md-9">
                  <section class="section extra-margins listing-section">
                    {#-----------------------MAIN-CONTENT-TITLE----------------------------#}
                    <h4 class="font-weight-bold">
                      <strong>
                        {% block title %} {% endblock %}
                      </strong>
                    </h4>
                    <hr class="red title-hr">
                    {#-----------------------MAIN-CONTENT-DESCRIPTION----------------------------#}
                    {% block content %}
                    {% endblock %}
                  </section>
                </div>
                {#----------------------END-MAIN-CONTENT-----------------------------#}
                {#-----------------------SIDEBAR-----------------------------#}
                <div class="col-12 col-md-3 col-lg-2 col-xl-2 widget-column
                  mt-0">
                  <section class="margeAside mb-5">
                    {#-----------------------SIDEBAR-TITLE-FIRST----------------------------#}
                    <h5 class="font-weight-bold aside">PARTENAIRES</h5>
                    {#-----------------------SIDEBAR-CONTENT-FIRST----------------------------#}
                    <div class="d-flex justify-content-center">
                      <div class="col-7 col-sm-4 col-md-10 col-lg-12 col-xl-12">
                        <div class="row mt-4">
                          {% for partenaires in session.partenaire %}
                          <a href="https://www.{{partenaires.link}}/"
                            class="text-hover"
                            target="_blank">
                            <div class="m-auto col-8 col-sm-8 col-md-12
                              col-lg-12">
                              {#-----------------------SIDEBAR-CONTENT-FIRST-IMAGE---------------------------#}
                              <div class="view overlay z-depth-1">
                                <img
                                  src="img/partenaire/{{partenaires.image}}"
                                  class="img-fluid"
                                  alt="Post">
                                <div class="mask rgba-white-slight waves-effect
                                  waves-light"></div>
                              </div>
                              {#-----------------------SIDEBAR-CONTENT-FIRST-LINK---------------------------#}
                              <div class="text-center mt-3">
                                <p class="mb-1 font-weight-bold">
                                  {{partenaires.legende}}
                                </p>
                              </div>
                            </div>
                          </a>
                          <br>
                          {% endfor %}
                        </div>
                        {#----------------------END-SIDEBAR-CONTENT-FIRST----------------------------#}
                      </div>
                    </div>
                    {#----------------------END-SIDEBAR-CONTENT-FIRST----------------------------#}
                  </section>
                  {#-----------------------BLOCK-SIDEBAR-CLASSIFICATION-YEARS-POST-------------------------#}
                  {% block archive %}
                  {% endblock %}
                  {#----------------------END-BLOCK-SIDEBAR-CLASSIFICATION-YEARS-POST-------------------------#}
                </div>
                {#-----------------------END-SIDEBAR-------------------------#}
              </div>
            </div>
          </div>
        </div>
      </main>
      {#-----------------------END-MAIN-------------------------#}
      {#----------------------FOOTER-------------------------#}
      <footer class="page-footer mt-4 pt-4">
        <div class="container-fluid">
          {#----------------------BEFOR-FOOTER-------------------------#}
          <div class="row text-center text-md-left mt-3 pb-3">
            <div class="container">
              <div class="row">
                {#-----------------------FOOTER-CONTENT-FIRST-COLONNE----------------------#}
                <div class="col-md-4 col-lg-4 col-xl-4 mr-auto mt-3">
                  {#-----------------------FOOTER-CONTENT-TITLE-----------------------#}
                  <h6 class="text-uppercase mb-4 font-weight-bold">LES 3 SOLDATS
                    BLANCS</h6>
                </div>
                {#-----------------------FOOTER-CONTENT-FIRST-COLONNE----------------------#}
                <hr class="w-100 clearfix d-md-none">
                {#-----------------------FOOTER-CONTENT-SECOND-COLONNE----------------------#}
                <div class="col-md-4 col-lg-2 col-xl-2 mr-auto mt-3">
                  <h6 class="text-uppercase mb-4 font-weight-bold">Menu</h6>
                  <p>
                    <a href="?p=page&title={{session.titlePageLink[7]}}">{{session.titlePage[7]}}</a>
                  </p>
                  <p>
                    <a href="?p=page&title={{session.titlePageLink[8]}}">{{session.titlePage[8]}}</a>
                  </p>
                  <p>
                    <a href="?p=page&title={{session.titlePageLink[9]}}">{{session.titlePage[9]}}</a>
                  </p>
                  <p>
                    <a href="?p=page&title={{session.titlePageLink[10]}}">{{session.titlePage[10]}}</a>
                  </p>
                </div>
                {#----------------------END-FOOTER-CONTENT-SECOND-COLONNE----------------------#}
                <hr class="w-100 clearfix d-md-none">
                {#----------------------FOOTER-CONTENT-FOUR-COLONNE----------------------#}
                <div class="col-md-4 col-lg-4 col-xl-4 mr-auto mt-3">
                  <h6 class="text-uppercase mb-4 font-weight-bold">Contact</h6>
                  <p>
                    <i class="fas fa-home mr-3"></i>9 Chemin de Charri√®re
                    Blanche, 69130 √âcully
                  </p>
                  <p>
                    <i class="fas fa-envelope mr-3"></i><a
                      href="http://projet5.marcalban.fr/?p=contact">Contact</a></p>
                  <p>
                    <i class="fas fa-phone mr-3"></i>06.33.97.76.76</p>
                  <p>
                    <i class="fas fa-print mr-3"></i><a
                      href="https://goo.gl/maps/AMS1tCe7sLAEG6XEA">Plan du site</a></p>
                </div>
                {#----------------------END-FOOTER-CONTENT-FOUR-COLONNE----------------------#}
              </div>
            </div>
          </div>
          {#---------------------END-BEFOR-FOOTER-------------------------#}
          {#---------------------BTN-TOP-------------------------#}
          <div><a id="cRetour" class="cInvisible" href="#haut"></a></div>
          {#----------------------END-BTN-TOP-------------------------#}
          {#----------------------TRUE-FOOTER-------------------------#}
          <div class="row py-3 d-flex align-items-center
            copyright-footer">
            <div class="container">
              <div class="row">
                {#---------------------FOOTER-COLONNE-ONE------------------------#}
                <div class="col-md-7 col-lg-8">
                  {#---------------------FOOTER-Copyright-------------------------#}
                  <p class="text-center text-md-left grey-text">
                    ¬© 2020 Copyright: <a href="#">LES3SOLDATSBLANCS.fr
                    </a>
                  </p>
                  {#--------------------END-FOOTER-Copyright-------------------------#}
                </div>
                {#--------------------END-FOOTER-COLONNE-ONE------------------------#}
                {#---------------------FOOTER-COLONNE-TWO------------------------#}
                <div class="col-md-5 col-lg-4 ml-lg-0">
                  {#---------------------FOOTER-Social-buttons-------------------------#}
                  <div class="social-section text-center mr-auto
                    text-md-left">
                    <ul class="list-unstyled list-inline">
                      {#---------------------FOOTER-Social-buttons-INSTAGRAM------------------------#}
                      <li class="list-inline-item">
                        <a class="btn-floating btn-sm
                          rgba-white-slight">
                          <i class="fab fa-instagram"></i>
                        </a>
                      </li>
                    </ul>
                  </div>
                  {#--------------------END-FOOTER-Social-buttons-------------------------#}
                </div>
                {#--------------------END-FOOTER-COLONNE-TWO------------------------#}
              </div>
            </div>
          </div>
          {#---------------------END-TRUE-FOOTER-------------------------#}
        </div>
      </footer>
      {#---------------------END-FOOTER-------------------------#}
      {#---------------------JAVASCRIPT-------------------------#}
      <script
        src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit"
        async defer>
    </script>
      {#---------------------JQUERY-------------------------#}
      <script type="text/javascript" src="js/MDB-Free/jquery.min.js"></script>
      {#---------------------TOOLTIPS-------------------------#}
      <script type="text/javascript" src="js/MDB-Free/popper.min.js"></script>
      {#---------------------BOOTSTRAP-JS-------------------------#}
      <script type="text/javascript"
        src="js/MDB-Free/bootstrap.min.js"></script>
      {#--------------------MDB-CORE-JS-------------------------#}
      <script type="text/javascript" src="js/MDB-Free/mdb.min.js"></script>
      {#--------------------BTNTOP-JS-------------------------#}
      <script>
      document.addEventListener('DOMContentLoaded', function() {
        window.onscroll = function(ev) {
          document.getElementById("cRetour").className = (window.pageYOffset > 100) ? "cVisible" : "cInvisible";
        };
      });
    </script>
      {#---------------------IMPORT-JS-------------------------#}
      {% block js %} {% endblock %}
    </body>
  </html>