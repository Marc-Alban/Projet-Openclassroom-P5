<!DOCTYPE html>
<html lang="en">

  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1,
      shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    {% block head %} {% endblock %}

    <!-- BM icon -->
    <link rel="icon" href="img/bm-favicon.ico" type="image/x-icon">

    <!-- Font Awesome -->
    <link rel="stylesheet"
      href="https://use.fontawesome.com/releases/v5.7.0/css/all.css">

    <!-- Bootstrap core CSS -->
    <link href="css/MDB-Free/bootstrap.min.css" rel="stylesheet">

    <!-- Material Design Bootstrap -->
    <link href="css/MDB-Free/mdb.min.css" rel="stylesheet">

    <!--Style -->
    <link href="css/frontend.css" rel="stylesheet">

    <script type="text/javascript">
    var verifyCallback = function (response) {
      alert(response);
    };
    var onloadCallback = function () {
      // Renders the HTML element with id 'example1' as a reCAPTCHA widget.
      // The id of the reCAPTCHA widget is assigned to 'widgetId1'.
      grecaptcha.render('captcha', {
        'sitekey': '6LdCHMIUAAAAAAWzKdTsmXFOtMohGlCSx7BXVM-9',
        'callback': verifyCallback,
        'theme': 'dark'
      });
    };
  </script>
  </head>

  <body class="fixed-sn homepage-v3">

    <!-- Navigation -->
    <header class="navbar-grey">

      <div class="container">
        <div class="row">
          <div class="col-lg-12 d-flex">
            <!-- Navbar brand -->
            <a class="navbar-brand mr-auto p-2" href="?page=home"><span>BM</span>Finance</a>
            {# Modal login #}
            <!-- User Login -->
            <div id="user" class="p-2" data-toggle="modal"
              data-target="#UserLog">
              <i class="fas fa-user"></i>
            </div>

            <!-- Modal -->
            <div class="modal fade" id="UserLog" tabindex="-1" role="dialog"
              aria-labelledby="TitleModalUser"
              aria-hidden="true">
              <div class="modal-dialog" role="document">
                <!--Content-->
                <div class="modal-content form-elegant">
                  <!--Header-->
                  <div class="modal-header text-center">
                    <h3 class="modal-title w-100 dark-grey-text font-weight-bold
                      my-3" id="TitleModalUser"><strong>Acces Administration</strong></h3>
                    <button type="button" class="close" data-dismiss="modal"
                      aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <!--Body-->
                  <div class="modal-body mx-4">
                    <form action="" method="POST">
                      <!--Body-->
                      <div class="md-form mb-5">
                        <input type="text" id="Form-User" class="form-control
                          validate" required>
                        <label data-error="wrong" data-success="right"
                          for="Form-User">Identifiant</label>
                      </div>

                      <div class="md-form pb-3">
                        <input type="password" id="Form-pass"
                          class="form-control
                          validate" required>
                        <label data-error="wrong" data-success="right"
                          for="Form-pass">Mot de passe</label>
                        <p class="font-small red-text d-flex
                          justify-content-start"><a href="lostPassword"
                            class="red-text
                            ml-1">*Mot de passe oublié</a></p>
                      </div>

                      <!-- POSTs back to the page's URL upon submit with a g-recaptcha-response POST parameter. -->
                      <div id="captcha"></div>

                      <div class="text-center mb-3">
                        <input type="submit" class="btn btn-block btn-red
                          btn-rounded z-depth-1a" value="Je me connect">
                      </div>
                    </form>


                  </div>
                </div>
                <!--/.Content-->
              </div>
            </div>
            <!-- Modal -->
            {# !modal #}
          </div>
        </div>
      </div>

      <div class="container">
        <div class="row">
          <!--Navbar-->
          <nav class="navbar navbar-expand-md">

            <!-- Collapse button -->
            <button class="navbar-toggler toggler-example" type="button"
              data-toggle="collapse"
              data-target="#navbarSupportedContent"
              aria-controls="navbarSupportedContent" aria-expanded="false"
              aria-label="Toggle navigation"><span class="dark-blue-text"><i
                  class="fas fa-bars fa-1x"></i></span></button>


            <!-- Collapsible content -->
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <!-- Links -->
              <ul class="navbar-nav mr-auto">
                <!-- Dropdown -->
                <li class="nav-item">
                  <a class="nav-link green" href="?page=home">Accueil</a>
                </li>

                <!-- Dropdown -->
                <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle blue"
                    id="navbarDropdownMenuLink1" data-toggle="dropdown"
                    aria-haspopup="true" aria-expanded="false">Blog</a>
                  <div class="dropdown-menu dropdown-primary"
                    aria-labelledby="navbarDropdownMenuLink">
                    <a class="dropdown-item"
                      href="?page=article&id=">Article de la
                      semaine</a>
                    <a class="dropdown-item" href="?page=listesArticles">Liste
                      des
                      articles
                      hebdomadaire</a>
                  </div>
                </li>

                <!-- Dropdown -->
                <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle orange"
                    id="navbarDropdownMenuLink1" data-toggle="dropdown"
                    aria-haspopup="true" aria-expanded="false">Qui sommes-nous ?</a>
                  <div class="dropdown-menu dropdown-primary"
                    aria-labelledby="navbarDropdownMenuLink">
                    <a class="dropdown-item" href="?page=page">Nos
                      valeurs</a>
                    <a class="dropdown-item" href="?page=page">Adresse</a>
                  </div>
                </li>

                <!-- Dropdown -->
                <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle yellow"
                    id="navbarDropdownMenuLink1" data-toggle="dropdown"
                    aria-haspopup="true" aria-expanded="false">Notre expertise</a>
                  <div class="dropdown-menu dropdown-primary"
                    aria-labelledby="navbarDropdownMenuLink">
                    <a class="dropdown-item" href="?page=page">Conseil
                      patrimonial</a>
                    <a class="dropdown-item" href="?page=page">Conseil
                      en
                      investissement
                      financier</a>
                    <a class="dropdown-item" href="?page=page">Conseil
                      en
                      investissement
                      immobilier</a>
                    <a class="dropdown-item" href="?page=page">Autres
                      solutions
                      d’investissement</a>
                  </div>
                </li>

                <!-- Dropdown -->
                <li class="nav-item">
                  <a class="nav-link blueStrong" href="?page=contact">Contact</a>
                </li>

              </ul>
              <!-- Links -->
            </div>
            <!-- Collapsible content -->


          </nav>
          <!--/.Navbar-->
        </div>
      </div>
    </header>
    <!--/ Navigation -->

    <!-- Main layout -->
    <main>
      <div class="container-fluid">

        <!-- Magazine -->
        <div class="row mt-5">
          <!-- container -->
          <div class="container">
            <div class="row">
              <!-- Main news -->
              <div class="col-xl-10 col-lg-10 col-md-9">
                <!--Section: Magazine posts-->
                <section class="section extra-margins listing-section">
                  <h4 class="font-weight-bold"><strong>
                      {% block title %} {% endblock %}
                    </strong></h4>
                  <hr class="red title-hr">
                  {% block content %} {% endblock %}
                </section>
                <!--/Section: Magazine posts-->
              </div>
              <!--/ Main news -->
              <!-- Sidebar -->

              <div class="col-12 col-md-3 col-lg-2 col-xl-2 widget-column mt-0">
                <section class="margeAside mb-5">
                  <h5 class="font-weight-bold aside">PARTENAIRES</h5>
                  <div class="d-flex justify-content-center">
                    <!--Grid row-->
                    <div class="col-7 col-sm-4 col-md-10 col-lg-12 col-xl-12">
                      <div class="row mt-4">
                        <!--Grid column-->
                        <div class="m-auto col-8 col-sm-8 col-md-12 col-lg-12">
                          <!--Image-->
                          <div class="view overlay z-depth-1">
                            <img src="img/logoWaldata.png" class="img-fluid"
                              alt="Post">
                            <a href="#!">
                              <div class="mask rgba-white-slight waves-effect
                                waves-light"></div>
                            </a>
                          </div>

                          <!--Post data-->
                          <div class="text-center mt-3">
                            <p class="mb-1 font-weight-bold">
                              <a href="#!" class="text-hover">Lien</a>
                            </p>
                          </div>
                        </div>
                        <!--Grid column-->
                        <div class="m-auto col-8 col-sm-8 col-md-12 col-lg-12">
                          <!--Image-->
                          <div class="view overlay z-depth-1">
                            <img src="img/quantalys.jpg" class="img-fluid"
                              alt="Post">
                            <a href="#!">
                              <div class="mask rgba-white-slight waves-effect
                                waves-light"></div>
                            </a>
                          </div>
                          <!--Post data-->
                          <div class="text-center mt-3">
                            <p class="mb-1 font-weight-bold">
                              <a href="#!" class="text-hover">Lien</a>
                            </p>
                          </div>
                        </div>
                        <!--Grid column-->
                      </div>
                      <!--Grid row-->
                    </div>
                  </div>
                </section>
                {% block archive %}
                {% endblock %}
              </div>

              <!--/ Sidebar -->
            </div>
          </div>
          <!-- /container -->
        </div>
        <!--/ Magazine -->
      </div>
    </main>
    <!--/ Main layout -->


