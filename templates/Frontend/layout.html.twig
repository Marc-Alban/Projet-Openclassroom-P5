<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1,
      shrink-to-fit=no">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  {% block head %} {% endblock %}

  <!-- BM icon -->
  <link rel="icon" href="img/bm-favicon.ico" type="image/x-icon">

  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css">

  <!-- Bootstrap core CSS -->
  <link href="css/MDB-Free/bootstrap.min.css" rel="stylesheet">

  <!-- Material Design Bootstrap -->
  <link href="css/MDB-Free/mdb.min.css" rel="stylesheet">

  <!--Style -->
  <link href="css/frontend.css" rel="stylesheet">

</head>

<body class="fixed-sn homepage-v3">

  <!-- Navigation -->
  <header class="navbar-grey">

    <div class="container">
      <div class="row">
        <div class="col-lg-12 d-flex">
          <!-- Navbar brand -->
          <a class="navbar-brand mr-auto p-2" href="#"><span>BM</span>Finance</a>
          {# Modal login #}
          <!-- User Login -->
          <div id="user" class="p-2" data-toggle="modal" data-target="#elegantModalForm">
            <i class="fas fa-user"></i>
          </div>

          <!-- Modal -->
          <div class="modal fade" id="elegantModalForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
            aria-hidden="true">
            <div class="modal-dialog" role="document">
              <!--Content-->
              <div class="modal-content form-elegant">
                <!--Header-->
                <div class="modal-header text-center">
                  <h3 class="modal-title w-100 dark-grey-text font-weight-bold
                      my-3" id="myModalLabel"><strong>Acces Administration</strong></h3>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
                <!--Body-->
                <div class="modal-body mx-4">
                  <!--Body-->
                  <div class="md-form mb-5">
                    <input type="text" id="Form-User" class="form-control
                        validate">
                    <label data-error="wrong" data-success="right" for="Form-User">Identifiant</label>
                  </div>

                  <div class="md-form pb-3">
                    <input type="password" id="Form-pass" class="form-control
                        validate">
                    <label data-error="wrong" data-success="right" for="Form-pass">Mot de passe</label>
                    <p class="font-small red-text d-flex
                        justify-content-start"><a href="#" class="red-text
                          ml-1">*Mot de passe oublié</a></p>
                  </div>

                  <div class="text-center mb-3">
                    <button type="button" class="btn btn-block btn-red
                        btn-rounded z-depth-1a">Je me connect</button>
                  </div>

                </div>
              </div>
              <!--/.Content-->
            </div>
          </div>
          <!-- Modal -->
          {# !modal #}
        </div>
      </div>
    </div>

    <div class="container">
      <div class="row">
        <!--Navbar-->
        <nav class="navbar navbar-expand-md">

          <!-- Collapse button -->
          <button class="navbar-toggler toggler-example" type="button" data-toggle="collapse"
            data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
            aria-label="Toggle navigation"><span class="dark-blue-text"><i
                class="fas fa-bars fa-1x"></i></span></button>


          <!-- Collapsible content -->
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <!-- Links -->
            <ul class="navbar-nav mr-auto">
              <!-- Dropdown -->
              <li class="nav-item">
                <a class="nav-link green">Accueil</a>
              </li>

              <!-- Dropdown -->
              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle blue" id="navbarDropdownMenuLink1" data-toggle="dropdown"
                  aria-haspopup="true" aria-expanded="false">Blog</a>
                <div class="dropdown-menu dropdown-primary" aria-labelledby="navbarDropdownMenuLink">
                  <a class="dropdown-item" href="#">Article de la semaine</a>
                  <a class="dropdown-item" href="#">Liste des articles
                    hebdomadaire</a>
                </div>
              </li>

              <!-- Dropdown -->
              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle orange" id="navbarDropdownMenuLink1" data-toggle="dropdown"
                  aria-haspopup="true" aria-expanded="false">Qui sommes-nous ?</a>
                <div class="dropdown-menu dropdown-primary" aria-labelledby="navbarDropdownMenuLink">
                  <a class="dropdown-item" href="#">Nos valeurs</a>
                  <a class="dropdown-item" href="#">Adresse</a>
                </div>
              </li>

              <!-- Dropdown -->
              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle yellow" id="navbarDropdownMenuLink1" data-toggle="dropdown"
                  aria-haspopup="true" aria-expanded="false">Notre expertise</a>
                <div class="dropdown-menu dropdown-primary" aria-labelledby="navbarDropdownMenuLink">
                  <a class="dropdown-item" href="#">Conseil patrimonial</a>
                  <a class="dropdown-item" href="#">Conseil en investissement
                    financier</a>
                  <a class="dropdown-item" href="#">Conseil en investissement
                    immobilier</a>
                  <a class="dropdown-item" href="#">Autres solutions
                    d’investissement</a>
                </div>
              </li>

              <!-- Dropdown -->
              <li class="nav-item">
                <a class="nav-link blueStrong">Contact</a>
              </li>

            </ul>
            <!-- Links -->
          </div>
          <!-- Collapsible content -->


        </nav>
        <!--/.Navbar-->
      </div>
    </div>
  </header>
  <!--/ Navigation -->

  <!-- Main layout -->
  <main>
    <div class="container-fluid">

      <!-- Magazine -->
      <div class="row mt-5">
        <!-- container -->
        <div class="container">
          <div class="row">
            <!-- Main news -->
            <div class="col-xl-9 col-lg-9 col-md-8">
              <!--Section: Magazine posts-->
              <section class="section extra-margins listing-section">
                <h4 class="font-weight-bold"><strong>
                    {% block title %} {% endblock %}
                  </strong></h4>
                <hr class="red title-hr">
                {% block content %} {% endblock %}
              </section>
              <!--/Section: Magazine posts-->
            </div>
            <!--/ Main news -->
            <!-- Sidebar -->
            <div class="col-xl-3 col-lg-3 col-md-4 widget-column mt-0">
              <section class="mb-5">
                <h4 class="font-weight-bold aside"><strong>PARTENAIRES</strong></h4>
                <!--Grid row-->
                <div class="row mt-4">

                  <!--Grid column-->
                  <div class="m-auto col-8 col-sm-8 col-md-12 col-lg-12">
                    <!--Image-->
                    <div class="view overlay z-depth-1">
                      <img src="img/logoWaldata.png" class="img-fluid" alt="Post">
                      <a href="#!">
                        <div class="mask rgba-white-slight waves-effect
                            waves-light"></div>
                      </a>
                    </div>

                    <!--Post data-->
                    <div class="text-center mt-3">
                      <p class="mb-1 font-weight-bold">
                        <a href="#!" class="text-hover">Lien</a>
                      </p>
                    </div>
                  </div>
                  <!--Grid column-->
                  <div class="m-auto col-8 col-sm-8 col-md-12 col-lg-12">
                    <!--Image-->
                    <div class="view overlay z-depth-1">
                      <img src="img/quantalys.jpg" class="img-fluid" alt="Post">
                      <a href="#!">
                        <div class="mask rgba-white-slight waves-effect
                            waves-light"></div>
                      </a>
                    </div>

                    <!--Post data-->
                    <div class="text-center mt-3">
                      <p class="mb-1 font-weight-bold">
                        <a href="#!" class="text-hover">Lien</a>
                      </p>
                    </div>

                  </div>
                  <!--Grid column-->

                </div>
                <!--Grid row-->

              </section>
              {% block archive %}
              {% endblock %}
            </div>
            <!--/ Sidebar -->
          </div>
        </div>
        <!-- /container -->
      </div>
      <!--/ Magazine -->
    </div>
  </main>
  <!--/ Main layout -->


  <!-- Footer -->
  <footer class="page-footer mt-4 pt-4">
    <!--Footer Links-->
    <div class="container-fluid">

      <!-- Footer links -->
      <div class="row text-center text-md-left mt-3 pb-3">

        <div class="container">
          <div class="row">
            <!--Grid column-->
            <div class="col-md-3 col-lg-3 col-xl-3 mr-auto mt-3">
              <h6 class="text-uppercase mb-4 font-weight-bold">BM Finance</h6>
              <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Veniam, quaerat quia molestias quos atque illo
                neque necessitatibus tempore. Consectetur, eos.</p>
            </div>
            <!--/.Grid column-->

            <hr class="w-100 clearfix d-md-none">

            <!--Grid column-->
            <div class="col-md-3 col-lg-2 col-xl-2 mr-auto mt-3">
              <h6 class="text-uppercase mb-4 font-weight-bold">Menu</h6>
              <p>
                <a href="#!">RGPD</a>
              </p>
              <p>
                <a href="#!">Conditions Générale</a>
              </p>
              <p>
                <a href="#!">FAQ</a>
              </p>
              <p>
                <a href="#!">Mentions Légales</a>
              </p>
            </div>
            <!--/.second column-->

            <hr class="w-100 clearfix d-md-none">

            <!--Grid column-->
            <div class="col-md-3 col-lg-2 col-xl-2 mr-auto mt-3">
              <h6 class="text-uppercase mb-4 font-weight-bold">Menu</h6>
              <p>
                <a href="#!">RGPD</a>
              </p>
              <p>
                <a href="#!">Conditions Générale</a>
              </p>
              <p>
                <a href="#!">FAQ</a>
              </p>
              <p>
                <a href="#!">Mentions Légales</a>
              </p>
            </div>
            <!--/.Third column-->

            <hr class="w-100 clearfix d-md-none">

            <!--Grid column-->
            <div class="col-md-4 col-lg-3 col-xl-3 mr-auto mt-3">
              <h6 class="text-uppercase mb-4 font-weight-bold">Contact</h6>
              <p>
                <i class="fas fa-home mr-3"></i>Adresse Postale</p>
              <p>
                <i class="fas fa-envelope mr-3"></i> contact@bmfinance.fr</p>
              <p>
                <i class="fas fa-phone mr-3"></i> + 01 234 567 88</p>
              <p>
                <i class="fas fa-print mr-3"></i> Plan du site</p>
            </div>
            <!--/.Grid column-->

          </div>
        </div>


      </div>
      <!-- Footer links -->

      <div class="row py-3 d-flex align-items-center copyright-footer">
        <div class="container">
          <div class="row">
            <!--Grid column-->
            <div class="col-md-7 col-lg-8">

              <!--Copyright-->
              <p class="text-center text-md-left grey-text">
                © 2019 Copyright: <a href="#"> BMFINANCE.fr
                </a>
              </p>
              <!--/.Copyright-->

            </div>
            <!--Grid column-->

            <!--Grid column-->
            <div class="col-md-5 col-lg-4 ml-lg-0">

              <!--Social buttons-->
              <div class="social-section text-center mr-auto text-md-left">
                <ul class="list-unstyled list-inline">
                  <li class="list-inline-item">
                    <a class="btn-floating btn-sm rgba-white-slight">
                      <i class="fab fa-instagram"></i>
                    </a>
                  </li>
                  <li class="list-inline-item">
                    <a class="btn-floating btn-sm rgba-white-slight">
                      <i class="fab fa-linkedin-in"></i>
                    </a>
                  </li>
                </ul>
              </div>
              <!--/.Social buttons-->

            </div>
            <!--Grid column-->

          </div>
        </div>
      </div>

    </div>
  </footer>
  <!-- Footer -->




  <!-- SCRIPTS -->

  <!-- JQuery -->
  <script type="text/javascript" src="js/MDB-Free/jquery.min.js"></script>
  <!-- Tooltips -->
  <script type="text/javascript" src="js/MDB-Free/popper.min.js"></script>
  <!-- Bootstrap core JavaScript -->
  <script type="text/javascript" src="js/MDB-Free/bootstrap.min.js"></script>
  <!-- MDB core JavaScript -->
  <script type="text/javascript" src="js/MDB-Free/mdb.min.js"></script>
</body>

</html>